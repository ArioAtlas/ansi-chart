  "devDependencies": {
    "@types/asciichart": "^1.5.8",
    "@types/clear": "^0.1.4",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
